{
  "name": "reminder-service",
  "version": "1.0.0",
  "description": "Event based microservice for managing reminders",
  "author": "Adisakshya Chauhan",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -n ",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n ",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "@nestjs/common": "^7.0.8",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.8",
    "@nestjs/platform-express": "^7.0.8",
    "@nestjs/typeorm": "^7.0.0",
    "aws-sdk": "^2.770.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.1",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.1.31",
    "pg": "^8.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "typeorm": "^0.2.25",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/swagger": "^4.5.3",
    "@nestjs/testing": "^7.0.8",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^12.7.8",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.7",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
